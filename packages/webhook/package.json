{
  "name": "@serverless-cd/webhook",
  "version": "0.0.1-alpha.8",
  "private": true,
  "description": "sevrerless-cd webhook",
  "main": "./lib/index.js",
  "scripts": {
    "start": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --outfile=example/lib/index.js --watch",
